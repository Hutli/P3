<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms"
					   xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
					   x:Class="OpenPlaylistApp.Views.VolumeView">
    <StackLayout >
		<Label Text="{Binding AverageVolume}"></Label>
        <Slider Minimum="0" Maximum="100" Value="{Binding SelectedVolume, Mode=OneWayToSource}"></Slider>
    </StackLayout>
	
</ContentView>